<div class="px-3 py-3 pt-md-5 pb-md-4">
        <h2 id="posters" tabindex="-1">Posters <a class="header-anchor" href="#posters" aria-hidden="true">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-link-45deg" viewBox="0 0 16 16"><path d="M4.715 6.542 3.343 7.914a3 3 0 1 0 4.243 4.243l1.828-1.829A3 3 0 0 0 8.586 5.5L8 6.086a1.002 1.002 0 0 0-.154.199 2 2 0 0 1 .861 3.337L6.88 11.45a2 2 0 1 1-2.83-2.83l.793-.792a4.018 4.018 0 0 1-.128-1.287z"/><path d="M6.586 4.672A3 3 0 0 0 7.414 9.5l.775-.776a2 2 0 0 1-.896-3.346L9.12 3.55a2 2 0 1 1 2.83 2.83l-.793.792c.112.42.155.855.128 1.287l1.372-1.372a3 3 0 1 0-4.243-4.243L6.586 4.672z"/></svg>
            </a>
        </h2>
    {% for presentation in posters %}
        <div class="talk py-4 border-bottom">
            <h3 id="{{ presentation.title | slugify}}" tabindex="-1">{{ presentation.title }} <a class="header-anchor" href="#{{ presentation.title | slugify}}" aria-hidden="true">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-link-45deg" viewBox="0 0 16 16"><path d="M4.715 6.542 3.343 7.914a3 3 0 1 0 4.243 4.243l1.828-1.829A3 3 0 0 0 8.586 5.5L8 6.086a1.002 1.002 0 0 0-.154.199 2 2 0 0 1 .861 3.337L6.88 11.45a2 2 0 1 1-2.83-2.83l.793-.792a4.018 4.018 0 0 1-.128-1.287z"/><path d="M6.586 4.672A3 3 0 0 0 7.414 9.5l.775-.776a2 2 0 0 1-.896-3.346L9.12 3.55a2 2 0 1 1 2.83 2.83l-.793.792c.112.42.155.855.128 1.287l1.372-1.372a3 3 0 1 0-4.243-4.243L6.586 4.672z"/></svg>
                </a>
            </h3>
            <p>{{ presentation.author }}</p>
            {%- if 'description' in presentation -%}

                <p class="d-inline-flex gap-1">
                    <a class="btn btn-primary" data-bs-toggle="collapse" href="#{{ presentation.title | slugify }}Description" role="button" aria-expanded="false" aria-controls="{{ presentation.title | slugify }}Description">
                        Read Proposal
                    </a>
                </p>
                <div class="collapse mb-3" id="{{ presentation.title | slugify }}Description">
                    <div class="card card-body">
                        {{ presentation.description | safe }}
                    </div>
                </div>
            {%- endif -%}
            <ul class="list-inline">
                {% if 'pdf_url' in presentation %}
                    <li class="list-inline-item">
                        <a class="btn btn-lg btn-danger" class="btn btn-lg btn-primary" href="{{ presentation.pdf_url }}">
                            <i class="bi bi-file-earmark-pdf"></i> PDF</a>
                    </li>
                {% endif %}
                {% if 'post_url' in presentation %}
                    <li class="list-inline-item">
                        <a class="btn btn-lg btn-danger" href="{{ presentation.post_url }}">
                            <i class="bi bi-rss-fill"></i> Blog Post</a>
                    </li>
                {% endif %}
            </ul>
        </div>
    {% endfor %}
</div>